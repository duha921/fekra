#include <iostream>
using namespace std;
int main ()
{
  int i, j;
  cout << "enter how many numbers you want to convert :";
  cin >> j;
  string res[j];

  int a[j];

  cout << "enter your number array: \n";

  for (i = 0; i < j; i++) // for loop to input numbers

    {

      cin >> a[i];

    }

  for (i = 0; i < j; i++) //for loop to check the contents and convert it to its written values

    {

      switch (a[i])
	{
	case 0:
	  res[i] = "zero";
	  break;
	case 1:
	  res[i] = "one";
	  break;
	case 2:
	  res[i] = "two";
	  break;
	case 3:
	  res[i] = "three";
	  break;
	case 4:
	  res[i] = "four";
	  break;
	case 5:
	  res[i] = "five";
	  break;
	case 6:
	  res[i] = "six";
	  break;
	case 7:
	  res[i] = "seven";
	  break;
	case 8:
	  res[i] = "eight";
	  break;
	case 9:
	  res[i] = "'nine'";
	  break;
	default:
	  cout << "Please try numbers between 0-9";
	  break;
	}
      cout << res[i] << " "; //desplaying output  
    }}
